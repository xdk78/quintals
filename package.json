    "build": "node fuse.js",
    "start": "cross-env ENV='development' electron .",
    "compile-win32": "cross-env ENV='production' npm run build && electron-builder -w -p always",
    "compile-darwin": "cross-env ENV='production' npm run build && electron-builder -m -p always",
    "compile-linux": "cross-env ENV='production' npm run build && electron-builder -l -p always",
    "compile": "cross-env ENV='production' npm run build && electron-builder",
    "lint": "tslint \"src/**/*.ts*\"",
    "lint-fix": "prettier --write \"src/**/*.ts*\" && tslint \"src/**/*.ts*\" --fix",
    "postinstall": "electron-builder install-app-deps"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ]
