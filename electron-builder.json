{
  "productName": "Quintals",
  "appId": "org.xdk78.quintals",
  "asar": true,
  "directories": {
    "output": "dist"
  },
  "files": ["build/**/*", "package.json"],
  "publish": "github",
  "linux": {
    "category": "Music",
    "target": [
      {
        "target": "deb",
        "arch": ["ia32", "x64"]
      },
      {
        "target": "AppImage",
        "arch": ["ia32", "x64"]
      }
    ]
  },
  "win": {
    "target": ["zip", "nsis"]
  },
  "mac": {
    "category": "public.app-category.music"
  }
}
